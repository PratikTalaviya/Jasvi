<%- include('../Header.ejs');-%>

    <div class="main-content container-fluid">
        <div class="page-title">
            <div class="row">
                <div class="col-12 col-md-6 order-md-1 order-last">
                    <h3 style="margin-bottom: 20px;">Specific Order </h3>
                </div>
            </div>
        </div>
        <%if (message.length> 0) {%>
            <div class="alert alert-dismissible"
                style="background:#5bc093;border-style: solid;  border-color:#55a682; border-width:1px; color: #fff;">
                <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
                <strong>Message!</strong>
                <%= message %>
            </div>
            <% } %>
                <%if(error.length> 0) {%>
                    <div class="alert alert-dismissible"
                        style="background:#fd7c7c;border-style: solid;  border-color:#e64b4b; border-width:1px; color: #fff;">
                        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
                        <strong>Error!</strong>
                        <%= error %>
                    </div>
                    <% } %>
                        <section class="section">
                            <div class="card">
                                <div class="card-body">
                                    <table class='table table-striped' id="category-table" style="width:100%">
                                        <thead class="head-tbl">
                                            <tr>
                                                <th>Channel Id</th>
                                                <th>Channel Name</th>
                                                <th>Channel Order Id</th>
                                                <th>Customer Name</th>
                                                <th>Customer Email</th>
                                                <th>Customer Phone</th>
                                                <th>Total</th>
                                                <th>Status</th>
                                                <th>Payment Method</th>
                                                <th>Awb</th>
                                                <th>status change</th>
                                                <!-- <th>Action</th> -->
                                            </tr>
                                        </thead>
                                        <tbody class="table-decorate">
                                            <tr>
                                                <td>
                                                    <%= myorderdata.channel_id %>
                                                </td>
                                                <td>
                                                    <%= myorderdata.channel_name %>
                                                </td>
                                                <td>
                                                    <%= myorderdata.channel_order_id %>
                                                </td>
                                                <td>
                                                    <%= myorderdata.customer_name %>
                                                </td>
                                                <td>
                                                    <%= myorderdata.customer_email %>
                                                </td>
                                                <td>
                                                    <%= myorderdata.customer_phone %>
                                                </td>
                                                <td>
                                                    <%= myorderdata.total %>
                                                </td>
                                                <td>
                                                    <%= myorderdata.status %>
                                                </td>
                                                <td>
                                                    <%= myorderdata.payment_method %>
                                                </td>
                                                <td>
                                                    <%= myorderdata.shipments.awb %>
                                                </td>
                                                <td>
                                                    <div class="btn-group">
                                                        <div class="dropdown">
                                                            <button class="btn btn-sm dropdown-toggle me-1"
                                                                type="button" id="dropdownMenuButton2"
                                                                data-bs-toggle="dropdown" aria-haspopup="true"
                                                                aria-expanded="false"
                                                                style="color: #000;background-color: #00000029;">
                                                                <%= myorderdata.status %>
                                                            </button>
                                                            <div class="dropdown-menu"
                                                                aria-labelledby="dropdownMenuButton2">
                                                                <a class="dropdown-item"
                                                                    href="/admin/CancelShippingOrder/<%=myorderdata._id%>">Canceled</a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!-- <form action="/CancelShippingOrder" method="POST">
                                        <div class="btn-group">
                                            <div class="dropdown" name="order_id">
                                            <button class="btn btn-sm dropdown-toggle me-1" type="button"
                                                id="dropdownMenuButton2" data-bs-toggle="dropdown" aria-haspopup="true"
                                                aria-expanded="false" style="color: #000;background-color: #00000029;"> -->
                                                    <!-- status -->
                                                    <!-- <%= myorderdata.status %>
                                            </button>
                                                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton2">
                                                    <div class="dropdown-item">Cancel</div> -->

                                                    <!-- </div>
                                                </div>
                                        </div>
                                    <button  type="submit">Submit</button>
                                </form> -->
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </section>
    </div>
    <script type="text/javascript">
        $(document).ready(function () {
            var table = $('#category-table').DataTable({
                dom: '<"dt-top-container"<l><"dt-center-in-div"B><f>r>t<"dt-filter-spacer"f><ip>',
                buttons: ['copy', 'pdf', 'csv', 'print', 'colvis']
            });
        });
    </script>

    <%- include('../Footer')-%>