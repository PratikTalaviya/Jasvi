<%- include('../Header.ejs');-%>

    <div class="main-content container-fluid">
        <div class="page-title mb-3">
            <div class="row">
                <div class="col-12 col-md-6 order-md-1 order-last">
                    <h3>Seller Address Insert</h3>
                </div>
                <div class="col-12 col-md-6 order-md-2 order-first">
                    <div class="d-flex justify-content-end">
                        <a href="/admin/AdminAddressInfo" class="btn btn-secondary"> Back</a>
                    </div>
                </div>
            </div>
        </div>
        <section id="basic-vertical-layouts">
            <div class="row match-height">
                <div class="col-md-12 col-sm-12 mx-auto">
                    <div class="card">
                        <div class="card-content">
                            <div class="card-body">
                                <form action="/admin/AdminAdressInsert" method="POST">
                                    <%if (message.length> 0) {%>
                                        <div class="alert alert-dismissible"
                                            style="background:#5bc093;border-style: solid;  border-color:#5bc093; border-width:1px; color: #fff;">
                                            <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
                                            <strong>Message!</strong>
                                            <%= message %>
                                        </div>
                                        <% } %>
                                            <%if(error.length> 0) {%>
                                                <div class="alert alert-dismissible"
                                                    style="background:#fd7c7c;border-style: solid;  border-color:#e64b4b; border-width:1px; color: #fff;">
                                                    <button type="button" class="btn-close"
                                                        data-bs-dismiss="alert"></button>
                                                    <strong>Error!</strong>
                                                    <%= error %>
                                                </div>
                                                <% } %>
                                                <div class="">
                                                    <div class="form-group position-relative has-icon-left">
                                                        <label for="username">channel Id:</label>
                                                        <div class="position-relative">
                                                            <input type="number     " name="channelId" class="form-control"
                                                            id="username">
                                                            <div class="form-control-icon">
                                                                <i data-feather="user"></i>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-group position-relative has-icon-left">
                                                        <label for="username">Admin name:</label>
                                                        <div class="position-relative">
                                                            <input type="text" name="Adminname" class="form-control"
                                                            id="username">
                                                            <div class="form-control-icon">
                                                                <i data-feather="user"></i>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-group position-relative has-icon-left">
                                                        <label for="username">Admin last Nname:</label>
                                                        <div class="position-relative">
                                                            <input type="text" name="AdminlastNname" class="form-control"
                                                            id="username">
                                                            <div class="form-control-icon">
                                                                <i data-feather="user"></i>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-group position-relative has-icon-left">
                                                        <label for="username">Admin address:</label>
                                                        <div class="position-relative">
                                                            <input type="text" name="Adminaddress" class="form-control"
                                                            id="username">
                                                            <div class="form-control-icon">
                                                                <i data-feather="user"></i>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-group position-relative has-icon-left">
                                                        <label for="username">Admin address 2:</label>
                                                        <div class="position-relative">
                                                            <input type="text" name="Adminaddress2" class="form-control"
                                                            id="username">
                                                            <div class="form-control-icon">
                                                                <i data-feather="user"></i>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-group position-relative has-icon-left">
                                                        <label for="username">Admin City:</label>
                                                        <div class="position-relative">
                                                            <input type="text" name="AdminCity" class="form-control"
                                                            id="username">
                                                            <div class="form-control-icon">
                                                                <i data-feather="user"></i>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-group position-relative has-icon-left">
                                                        <label for="username">Admin Country:</label>
                                                        <div class="position-relative">
                                                            <input type="text" name="AdminCountry" class="form-control"
                                                            id="username">
                                                            <div class="form-control-icon">
                                                                <i data-feather="user"></i>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-group position-relative has-icon-left">
                                                        <label for="username">Admin Pin code:</label>
                                                        <div class="position-relative">
                                                            <input type="number" name="AdminPincode" class="form-control"
                                                            id="username">
                                                            <div class="form-control-icon">
                                                                <i data-feather="user"></i>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-group position-relative has-icon-left">
                                                        <label for="username">Admin State:</label>
                                                        <div class="position-relative">
                                                            <input type="text" name="AdminState" class="form-control"
                                                            id="username">
                                                            <div class="form-control-icon">
                                                                <i data-feather="user"></i>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-group position-relative has-icon-left">
                                                        <label for="username">Admin Email:</label>
                                                        <div class="position-relative">
                                                            <input type="text" name="AdminEmail" class="form-control"
                                                            id="username">
                                                            <div class="form-control-icon">
                                                                <i data-feather="user"></i>
                                                            </div>
                                                        </div>
                                                    </div>
                                                 
                                                    <div class="form-group  position-relative has-icon-left">
                                                        <label for="username">Admin phone:</label>
                                                        <div class="position-relative">
                                                            <input type="number" name="Adminphone" class="form-control"
                                                            id="username">
                                                            <div class="form-control-icon">
                                                                <i data-feather="user"></i>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                    


                                                    <div class="clearfix">
                                                        <button class="btn btn-primary float-end">Submit</button>
                                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>


    <%- include('../Footer')-%>