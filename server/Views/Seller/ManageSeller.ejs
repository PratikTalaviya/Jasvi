<%- include('../Header.ejs');-%>

    <div class="main-content container-fluid">
        <div class="page-title">
            <div class="row">
                <div class="col-12 col-md-6 order-md-1 order-last">
                    <h3 style="margin-bottom: 20px;">Seller Address</h3>
                </div>
                <div class="col-12 col-md-6 order-md-2 order-first">
                    <div class="d-flex justify-content-end">
                        <a href="/admin/InsertAdminAddress" class="btn btn-secondary"> Add</a>
                    </div>
                </div>
            </div>
        </div>
        <%if (message.length> 0) {%>
            <div class="alert alert-dismissible"
                style="background:#5bc093;border-style: solid;  border-color:#55a682; border-width:1px; color: #fff;">
                <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
                <strong>Success! </strong>
                <%= message %>
            </div>
            <% } %>
                <%if(error.length> 0) {%>
                    <div class="alert alert-dismissible"
                        style="background:#fd7c7c;border-style: solid;  border-color:#e64b4b; border-width:1px; color: #fff;">
                        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
                        <strong>Error!</strong>
                        <%= error %>
                    </div>
                    <% } %>
                        <section class="section">
                            <div class="card">
                                <div class="card-body">
                                    <table class='table table-striped' id="category-table" style="width:100%">
                                        <thead class="head-tbl">
                                            <tr>

                                                <th>channel Id</th>
                                                <th>Admin name</th>
                                                <th>Admin last Nname</th>
                                                <th>Admin address</th>
                                                <th>Admin address 2</th>
                                                <th>Admin City</th>
                                                <th>Admin Country</th>
                                                <th>Admin Pincode</th>
                                                <th>Admin State</th>
                                                <th>Admin Email</th>
                                                <th>Admin phone</th>
                                            </tr>
                                        </thead>
                                        <tbody class="table-decorate">
                                            <% AdminData.forEach(function(social){ %>
                                                <tr>

                                                    <td>
                                                        <%= social.channelId %>
                                                    </td>
                                                    <td>
                                                        <%= social.Adminname %>
                                                    </td>
                                                    <td>
                                                        <%= social.AdminlastNname %>
                                                    </td>
                                                    <td>
                                                        <%= social.Adminaddress %>
                                                    </td>
                                                    <td>
                                                        <%= social.Adminaddress2 %>
                                                    </td>
                                                    <td>
                                                        <%= social.AdminCity %>
                                                    </td>
                                                    <td>
                                                        <%= social.AdminCountry %>
                                                    </td>
                                                    <td>
                                                        <%= social.AdminPincode %>
                                                    </td>
                                                    <td>
                                                        <%= social.AdminState %>
                                                    </td>
                                                    <td>
                                                        <%= social.AdminEmail %>
                                                    </td>
                                                    <td>
                                                        <%= social.Adminphone %>
                                                    </td>
                                                    <td>
                                                        <a href="/admin/EditSellerPage/<%=social._id%>"
                                                            class="btn icon btn-primary"><i data-feather="edit"></i></a>
                                                        <a href="/admin/SellerSoftDelete/<%=social._id%>"
                                                            class="btn icon btn-danger"><i data-feather="delete"
                                                                width="20"></i> </a>
                                                    </td>
                                                </tr>
                                                <% }) %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </section>
    </div>
    <script type="text/javascript">
        $(document).ready(function () {
            var table = $('#category-table').DataTable({
                dom: '<"dt-top-container"<l><"dt-center-in-div"B><f>r>t<"dt-filter-spacer"f><ip>',
                buttons: ['copy', 'pdf', 'csv', 'print', 'colvis']
            });
        });
    </script>

    <%- include('../Footer')-%>