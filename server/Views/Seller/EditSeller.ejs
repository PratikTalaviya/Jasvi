<%- include('../Header.ejs');-%>

    <div class="main-content container-fluid">
        <div class="page-title mb-3">
            <div class="row">
                <div class="col-12 col-md-6 order-md-1 order-last">
                    <h3>Edit Seller </h3>
                </div>
                <div class="col-12 col-md-6 order-md-2 order-first">
                    <div class="d-flex justify-content-end">
                        <a href="/admin/AdminAddressInfo" class="btn btn-secondary"> Back</a>
                    </div>
                </div>
            </div>
        </div>
        <section id="basic-vertical-layouts">
            <div class="row match-height">
                <div class="col-md-12 col-sm-12 mx-auto">
                    <div class="card">
                        <div class="card-content">
                            <div class="card-body">

                                <form class="form form-vertical" action="/admin/EditSellerActions/<%=data._id%>"
                                    method="POST">
                                    <%if (message.length> 0) {%>
                                        <div class="alert alert-dismissible"
                                            style="background:#5bc093;border-style: solid;  border-color:#55a682; border-width:1px; color: #fff;">
                                            <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
                                            <strong>Message!</strong>
                                            <%= message %>
                                        </div>
                                        <% } %>
                                            <%if(error.length> 0) {%>
                                                <div class="alert alert-dismissible"
                                                    style="background:#fd7c7c;border-style: solid;  border-color:#e64b4b; border-width:1px; color: #fff;">
                                                    <button type="button" class="btn-close"
                                                        data-bs-dismiss="alert"></button>
                                                    <strong>Error!</strong>
                                                    <%= error %>
                                                </div>
                                                <% } %>
                                                    <div class="form-body">
                                                        <div class="row flex-column align-content-around">
                                                            <div class="col-12 mx-auto">
                                                                <div class="form-group">
                                                                    <label for="first-name-vertical">channelId</label>
                                                                    <input type="number" id="first-name-vertical"
                                                                        class="form-control" name="channelId"
                                                                        value="<%= data.channelId %>">
                                                                </div>
                                                            </div>
                                                            <div class="col-12 mx-auto">
                                                                <div class="form-group">
                                                                    <label for="first-name-vertical">Adminname</label>
                                                                    <input type="text" id="first-name-vertical"
                                                                        class="form-control" name="Adminname"
                                                                        value="<%= data.Adminname %>">
                                                                </div>
                                                            </div>
                                                            <div class="col-12 mx-auto">
                                                                <div class="form-group">
                                                                    <label for="first-name-vertical">AdminlastNname</label>
                                                                    <input type="text" id="first-name-vertical"
                                                                        class="form-control" name="AdminlastNname"
                                                                        value="<%= data.AdminlastNname %>">
                                                                </div>
                                                            </div>
                                                            <div class="col-12 mx-auto">
                                                                <div class="form-group">
                                                                    <label for="first-name-vertical">Adminaddress</label>
                                                                    <input type="text" id="first-name-vertical"
                                                                        class="form-control" name="Adminaddress"
                                                                        value="<%= data.Adminaddress %>">
                                                                </div>
                                                            </div>
                                                            <div class="col-12 mx-auto">
                                                                <div class="form-group">
                                                                    <label for="first-name-vertical">Adminaddress2</label>
                                                                    <input type="text" id="first-name-vertical"
                                                                        class="form-control" name="Adminaddress2"
                                                                        value="<%= data.Adminaddress2 %>">
                                                                </div>
                                                            </div>
                                                            <div class="col-12 mx-auto">
                                                                <div class="form-group">
                                                                    <label for="first-name-vertical">AdminCity</label>
                                                                    <input type="text" id="first-name-vertical"
                                                                        class="form-control" name="AdminCity"
                                                                        value="<%= data.AdminCity %>">
                                                                </div>
                                                            </div>
                                                            <div class="col-12 mx-auto">
                                                                <div class="form-group">
                                                                    <label for="first-name-vertical">AdminCountry</label>
                                                                    <input type="text" id="first-name-vertical"
                                                                        class="form-control" name="AdminCountry"
                                                                        value="<%= data.AdminCountry %>">
                                                                </div>
                                                            </div>
                                                            <div class="col-12 mx-auto">
                                                                <div class="form-group">
                                                                    <label for="first-name-vertical">AdminPincode</label>
                                                                    <input type="number" id="first-name-vertical"
                                                                        class="form-control" name="AdminPincode"
                                                                        value="<%= data.AdminPincode %>">
                                                                </div>
                                                            </div>
                                                            <div class="col-12 mx-auto">
                                                                <div class="form-group">
                                                                    <label for="first-name-vertical">AdminState</label>
                                                                    <input type="text" id="first-name-vertical"
                                                                        class="form-control" name="AdminState"
                                                                        value="<%= data.AdminState %>">
                                                                </div>
                                                            </div>
                                                            <div class="col-12 mx-auto">
                                                                <div class="form-group">
                                                                    <label for="first-name-vertical">AdminEmail</label>
                                                                    <input type="text" id="first-name-vertical"
                                                                        class="form-control" name="AdminEmail"
                                                                        value="<%= data.AdminEmail %>">
                                                                </div>
                                                            </div>
                                                            
                                                            <div class="col-12 mx-auto">
                                                                <div class="form-group">
                                                                    <label for="first-name-vertical">Adminphone</label>
                                                                    <input type="number" id="first-name-vertical"
                                                                        class="form-control" name="Adminphone"
                                                                        value="<%= data.Adminphone %>">
                                                                </div>
                                                            </div>

                                                            <div
                                                                class="col-12 mx-auto d-flex justify-content-center mt-3">
                                                                <button type="submit"
                                                                    class="btn btn-primary me-1 mb-1">Submit</button>
                                                            </div>
                                                        </div>
                                                    </div>
                                </form>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>

    <%- include('../Footer')-%>